<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('static/header.ejs');%>
</head>

<body>
    <div class="container">
        <%- include('static/adminNavigation.ejs');%>
            <br><br>
            <h1>PAGINA EN CONSTRUCCION</h1>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Nombre</th>
                        <th scope="col">Apellido</th>
                        <th scope="col">Documento</th>
                        <th scope="col">Eliminar</th>
                    </tr>
                </thead>
                <tbody>
                    <% clients.forEach(function(client){ %>
                    <tr>
                        <td><%- client.nombre %></td>
                        <td><%- client.apellido %></td>
                        <td><%- client.documento %></td>
                        <td><a href="/deleteClient/<%- client.documento %>" class="btn btn-danger">ELIMINAR</a></td>
                    </tr>
                    <% }) %>
                </tbody>
            </table>
    </div>
    <%- include('static/footer.ejs');%>
</body>

</html>